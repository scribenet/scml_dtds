<!-- ============================================================ -->
<!--                             Root                             -->
<!-- ============================================================ -->

  <!ELEMENT scml ( division+ | book )+ >
  <!ATTLIST scml
    %coreattrs;
    bver  CDATA  #IMPLIED
  >

<!-- ============================================================ -->
<!--                    Major Structure Styles                    -->
<!-- ============================================================ -->

  <!ELEMENT book ( chapter )+ >
  <!ATTLIST book
    %coreattrs;
  >

  <!ELEMENT chapter ( cover | (%block_styles; | %paragraph_styles;)+ ) >
  <!ATTLIST chapter
    %coreattrs;
  >

  <!ELEMENT division (book+) >
  <!ATTLIST division
    %coreattrs;
  >

<!-- ============================================================ -->
