<!-- ============================================================ -->
<!--                 Specialized Structure Styles                 -->
<!-- ============================================================ -->

  <!ELEMENT cell (%block_styles; | %paragraph_styles;)* >
  <!ATTLIST cell
    %coreattrs;
    colspan  CDATA  #IMPLIED
    rowspan  CDATA  #IMPLIED
    align  CDATA  #IMPLIED
  >

  <!ELEMENT cover ( fig ) >
  <!ATTLIST cover
    %coreattrs;
  >

  <!ELEMENT table ( tr+ ) >
  <!ATTLIST table
    %coreattrs;
    width  CDATA  #IMPLIED
    style  CDATA  #IMPLIED
    border  CDATA  #IMPLIED
    orient  CDATA  #IMPLIED
  >

  <!ELEMENT tr ( cell+ ) >
  <!ATTLIST tr
    %coreattrs;
    align  CDATA  #IMPLIED
    valign  CDATA  #IMPLIED
  >

