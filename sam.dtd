<!-- ============================================================ -->
<!--                             Root                             -->
<!-- ============================================================ -->

  <!ELEMENT sam (%paragraph_styles; | table | footnote | endnote)+ >
  <!ATTLIST sam
    %coreattrs;
    bver  CDATA  #IMPLIED
  >

<!-- ============================================================ -->
<!--                         Block Styles                         -->
<!-- ============================================================ -->

  <!ELEMENT endnote (%paragraph_styles;)* >
  <!ATTLIST endnote
    %coreattrs;
  >

  <!ELEMENT footnote (%paragraph_styles;)* >
  <!ATTLIST footnote
    %coreattrs;
  >

<!-- ============================================================ -->
<!--                 Specialized Structure Styles                 -->
<!-- ============================================================ -->

  <!ELEMENT table ( tr+ ) >
  <!ATTLIST table
    %coreattrs;
    width  CDATA  #IMPLIED
    style  CDATA  #IMPLIED
    border  CDATA  #IMPLIED
    orient  CDATA  #IMPLIED
  >

  <!ELEMENT tr ( cell+ ) >
  <!ATTLIST tr
    %coreattrs;
    align  CDATA  #IMPLIED
    valign  CDATA  #IMPLIED
  >

  <!ELEMENT cell (%paragraph_styles;)* >
  <!ATTLIST cell
    %coreattrs;
    colspan  CDATA  #IMPLIED
    rowspan  CDATA  #IMPLIED
    align  CDATA  #IMPLIED
  >

